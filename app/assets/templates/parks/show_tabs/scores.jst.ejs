<tbody>
  <tr>
    <td>Trip Advisor Rating: </td>
    <td class="rating">
      <div class="progress <%= park.scoreClass(park.escape('tripadv_rating')) %>">
        <div class="bar" style="width: <%= park.escape('tripadv_rating') %>%"></div>
      </div>
    </td>
  </tr>
  <tr>
    <td>Weather Score: </td>
    <td class="rating">
      <div class="progress <%= park.scoreClass(park.weather().score) %>">
        <div class="bar" style="width: <%= park.weather().score %>%"></div>
      </div>
    </td>
  </tr>
</tbody>