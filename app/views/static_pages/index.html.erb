<div id="content"></div>

<script>
  $(function () {
    <% if current_user %>
      ThemeParkList.CURRENT_USER = {
        id: <%= current_user.id %>,
        username: "<%= current_user.username %>",
        email: "<%= current_user.email %>"
      }
    <% else %>
      ThemeParkList.CURRENT_USER = undefined;
    <% end %>
    ThemeParkList.PARK_NAMES = <%= Park.pluck(:name).to_json.html_safe %>;
    ThemeParkList.initialize();
  })
</script>